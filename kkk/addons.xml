<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.abc_iview" name="ABC iView" provider-name="Andy Botting" version="1.6.4">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension library="default.py" point="xbmc.python.pluginsource" provides="video">
    <provides>video</provides>
  </extension>
  <extension library="default.py" point="xbmc.addon.metadata">
    <summary>ABC iView add-on by Andy Botting</summary>
    <description>iView offers full-length programs as seen on ABC TV, and iView originals ranging from arts to anime. Programs are added every day; most programs are available for 14 days. You must be located in Australia to access content. NOTE: This will count towards your Internet usage quota.</description>
    <provides>video</provides>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.kkk" name="kkk" version="1.1.3" provider-name="netxeon-country">
	<extension point="xbmc.addon.repository">
		<info compressed="false">https://raw.githubusercontent.com/tustxk/AddOnRepo/master/kkk/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/tustxk/AddOnRepo/master/kkk/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/tustxk/AddOnRepo/master/kkk</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>XBMC Addons for kkk Only</summary>
		<description>There is no description.</description>
		<disclaimer>The owners and submitters to tis repository do not host or distribute any of the content displayed by these addons nor do they have any affiliation with the content providers.</disclaimer>
		<platform>all</platform>
	</extension>
</addon>

