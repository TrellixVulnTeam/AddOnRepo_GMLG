<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.Australian" name="Australian" version="1.0.0" provider-name="netxeon-country">
	<extension point="xbmc.addon.repository">
		<info compressed="false">https://raw.githubusercontent.com/tustxk/AddOnRepo/master/Australian/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/tustxk/AddOnRepo/master/Australian/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/tustxk/AddOnRepo/master/Australian</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>XBMC Addons for Australian Only</summary>
		<description>There is no description.</description>
		<disclaimer>The owners and submitters to tis repository do not host or distribute any of the content displayed by these addons nor do they have any affiliation with the content providers.</disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="service.repo.check"
       name="repocheck"
       version="0.1"
       provider-name="BIT">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
  </requires>
  <extension point="xbmc.service"
            library="default.py" start="startup">
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
  </extension>
</addon>
</addons>
